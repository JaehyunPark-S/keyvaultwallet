            "post": {
                "tags": [
                    "Wallet"
                ],
                "summary": "Return signature",
                "description": "Returns signature, value, keyName",
                "operationId": "sign",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "keyName",
                        "in": "formData",
                        "description": "Input your keyName",
                        "required": true,
                        "type": "string",
                        "default": "testkey"
                    },
                    {
                        "name": "keyVersion",
                        "in": "formData",
                        "description": "Input your keyVersion",
                        "required": true,
                        "type": "string",
                        "default": "930f183a6f9f4bab99b32206e008eedd"
                    },
                    {
                        "name": "message",
                        "in": "formData",
                        "description": "Input your privateKey",
                        "required": true,
                        "type": "string",
                        "default": "Vgyk4k6SQnGJ2rMtR19XTBYBik3pfF9+aJXAfjk7hoY="
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "schema": {
                            "type": "string"
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
